    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/moment": "^1.2.0",
    "@nuxtjs/pwa": "latest",
    "bulma-modal-fx": "^1.1.1",
    "glob": "^7.1.4",
    "node-snackbar": "^0.1.15",
    "tippy.js": "^4.3.4",
    "webhook-discord": "^3.2.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "@nuxtjs/markdownit": "^1.2.5",
