{
  "name": "Mattaio-Website",
  "version": "1.0.0",
  "description": "Mattèo Gauthier personnal website, find out some blog-posts, tutorials, et",
  "keywords": [
    "Mattèo Gauthier",
    "Mattèo",
    "semoule.fr",
    "semoule",
    "mattaio-website",
    "personnal website"
  ],
  "author": "Mattèo Gauthier",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "start": "nuxt start",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "build:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
    "top": "node test.js",
    "generate:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "direct-deploy": "npm run typical-deploy && npm run ghpages-deploy",
    "typical-deploy": "nuxt generate && surge dist/ mattaio.surge.sh && now",
    "ghpages-deploy": "npm run generate:gh-pages && npm run gh-pages",
    "gh-pages": "cross-env DEPLOY_ENV=GH_PAGES push-dir --dir=dist --branch=gh-pages --verbose"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
